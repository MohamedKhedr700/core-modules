{
    "name": "raid/core-modules",
    "description": "Raid Core Modules",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "minimum-stability": "dev",
    "require": {
        "php": "^8.2",
        "raid/core-command": "dev-main",
        "nwidart/laravel-modules": "^10.0@dev",
        "laravel/pint": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Raid\\Core\\Modules\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Raid\\Core\\Modules\\Providers\\ModuleServiceProvider"
            ]
        }
    },
    "scripts": {
        "pint": [
            "./vendor/bin/pint --config pint.json "
        ]
    },
    "authors": [
        {
            "name": "mohamedkhedr700",
            "email": "mohamedkhedr700@gmail.com"
        }
    ]
}
